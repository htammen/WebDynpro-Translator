  private void setInPlugParameter(){
  	// Fuegt eine leere Ausschreibung zum Context hinzu und setzt die LeadSelection auf diese
  	// Ausschreibung, damit das Eingabefeld leer ist
	IAusschreibungenElement tempAus = wdContext.nodeAusschreibungen().createAusschreibungenElement(new Ausschreibung());
	wdContext.nodeAusschreibungen().addElement( tempAus );
	wdContext.nodeAusschreibungen().setLeadSelection( wdContext.nodeAusschreibungen().size() -1 );	
  }
  
  private void resetSelection(){
		IInputAusschreibungsplattformReportGetListElement inputElem = wdContext.currentInputAusschreibungsplattformReportGetListElement();
		inputElem.setStatusOffen( Boolean.FALSE );
		inputElem.setStatusGespeichert( Boolean.FALSE );
		inputElem.setStatusFreigegeben( Boolean.FALSE );
		inputElem.setStatusAbgesagt( Boolean.FALSE );
		inputElem.setStatusBestellungErstellt( Boolean.FALSE );
		inputElem.setStatusZurueckgezogen( Boolean.FALSE );
		inputElem.setAngebotsartAbgebot( Boolean.FALSE );
		inputElem.setAngebotsartAnfrage( Boolean.FALSE );
		inputElem.setEinkaeufergruppe("");		
  }
  
  //@@end
}
