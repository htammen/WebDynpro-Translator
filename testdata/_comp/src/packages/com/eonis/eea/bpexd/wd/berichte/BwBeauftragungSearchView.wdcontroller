    //@@begin onActionSearchBeauftragungBySelection(ServerEvent)
    
    // Fehler wenn keine Checkbox ausgewaehlt wurde 
    IInputGetBestellungenReportBySelectionElement iElem = 
    	wdContext.nodeInputGetBestellungenReportBySelection().currentInputGetBestellungenReportBySelectionElement();
    if (!iElem.getTypeAbrufe() && !iElem.getTypeBestellungen()){
		wdComponentAPI.getMessageManager().reportMessage( IMessageBerichte.KEINE_BEAUFTRAGUNG, null, false ); 
    }
    else if (!iElem.getStatusAbgeschlossen() && !iElem.getStatusOffen()){
		wdComponentAPI.getMessageManager().reportMessage( IMessageBerichte.KEINE_BEAUFTRAGUNG, null, false );
    }
    else{
		wdThis.wdGetBerichteController().getBestellungenReportBySelection();
		wdThis.wdFirePlugToBeauftragungsList();
    }    
    //@@end
  }

