  //@@begin javadoc:getAusschreibungenAusschreibungsplattformReportGetListStatusText(IPrivateBwAusplattformResultView.IAusschreibungenAusschreibungsplattformReportGetListElement)
  /**
   *  Declared getter method for attribute statusText of node ausschreibungenAusschreibungsplattformReportGetList
   *  @param element the element requested for the value
   *  @return the calculated value for attribute statusText
   */
  //@@end
  public java.lang.String getAusschreibungenAusschreibungsplattformReportGetListStatusText(IPrivateBwAusplattformResultView.IAusschreibungenAusschreibungsplattformReportGetListElement element)
  {
    //@@begin getAusschreibungenAusschreibungsplattformReportGetListStatusText(IPrivateBwAusplattformResultView.IAusschreibungenAusschreibungsplattformReportGetListElement)
    String retValue = "";
    
    int r3Status = element.getPosStatus();
    if ( r3Status == Ausschreibung.POS_STATUS_NEU ){
    	retValue = wdComponentAPI.getTextAccessor().getText("AusplattStatusNeu");
    } else if ( r3Status == Ausschreibung.POS_STATUS_GESICHERT ){
			retValue = wdComponentAPI.getTextAccessor().getText("AusplattStatusGesichert");
    }	else if ( r3Status == Ausschreibung.POS_STATUS_FREIGEGEBEN ){
			retValue = wdComponentAPI.getTextAccessor().getText("AusplattStatusFreigegeben");
		} else if ( r3Status == Ausschreibung.POS_STATUS_ZURUECKGEZOGEN ){
			retValue = wdComponentAPI.getTextAccessor().getText("AusplattStatusZurueckgezogen");
		}	else if ( r3Status == Ausschreibung.POS_STATUS_ABGESAGT ){
			retValue = wdComponentAPI.getTextAccessor().getText("AusplattStatusAbgesagt");
		}	else if ( r3Status == Ausschreibung.POS_STATUS_BESTELLT ){
			retValue = wdComponentAPI.getTextAccessor().getText("AusplattStatusBestellt");
		}
    return retValue;
    //@@end
  }

