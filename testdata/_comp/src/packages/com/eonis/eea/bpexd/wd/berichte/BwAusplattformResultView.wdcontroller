  //@@begin javadoc:onActionPrintReport(ServerEvent)
  /** Declared validating event handler. */
  //@@end
  public void onActionPrintReport(com.sap.tc.webdynpro.progmodel.api.IWDCustomEvent wdEvent )
  {
    //@@begin onActionPrintReport(ServerEvent)
    
		//Hier wird die Ausschreibungsnummer in den InputNode gefuellt
		wdContext.currentAusschreibungenElement().setSubmissionsnummer( wdContext.currentAusschreibungenAusschreibungsplattformReportGetListElement().getSubmissionsnummer() );		
		wdThis.wdGetBerichteController().ladeAusschreibung();
		
		//	Kopieren der  Daten
		wdThis.wdGetBerichteController().copyAbgAnfToDataSource();
		// Image URL generieren
		wdThis.wdGetBerichteController().generateServletURL( );
	
		// Abgebot
		if ( wdContext.currentAusschreibungElement().getAnfragetyp() == Ausschreibung.ANFRAGETYP_ABGEBOT ){
			
			// Popup-Fenster fuer Abgebote Drucken erzeugen und anzeigen	
			IWDWindowInfo popupAusschreibungAbgPrintViewInfo = wdComponentAPI.getComponentInfo().findInWindows( WND_AUSCHREIBUNG_ABG_PRINT );
			if( popupAusschreibungAbgPrintViewInfo != null ) {
				IWDWindow popupAusschreibungAbgPrintView = wdComponentAPI.getWindowManager().createWindow( popupAusschreibungAbgPrintViewInfo, true );
				popupAusschreibungAbgPrintView.setWindowPosition( WDWindowPos.CENTER );
				popupAusschreibungAbgPrintView.open();
				wdContext.currentComponentDataPrintPopupElement().setPopupAusschreibungPrintView( popupAusschreibungAbgPrintView );
			}
		}
		
		// Anfrage		
		if ( wdContext.currentAusschreibungElement().getAnfragetyp() == Ausschreibung.ANFRAGETYP_ANFRAGE ){	
			
			//Popup-Fenster fuer Anfragen Drucken erzeugen und anzeigen	
			IWDWindowInfo popupAusschreibungAnfPrintViewInfo = wdComponentAPI.getComponentInfo().findInWindows( WND_AUSCHREIBUNG_ANF_PRINT );
			if( popupAusschreibungAnfPrintViewInfo != null ) {
				IWDWindow popupAusschreibungAnfPrintView = wdComponentAPI.getWindowManager().createWindow( popupAusschreibungAnfPrintViewInfo, true );
				popupAusschreibungAnfPrintView.setWindowPosition( WDWindowPos.CENTER );
				popupAusschreibungAnfPrintView.open();
				wdContext.currentComponentDataPrintPopupElement().setPopupAusschreibungPrintView( popupAusschreibungAnfPrintView );
			}
		}
    //@@end
  }

  //@@begin javadoc:onActionToDetailView(ServerEvent)
  /** Declared validating event handler. */
  //@@end
  public void onActionToDetailView(com.sap.tc.webdynpro.progmodel.api.IWDCustomEvent wdEvent )
  {
    //@@begin onActionToDetailView(ServerEvent)
    
	// Kopieren der Submissionsnummer in das Input-Model-Objekt von AusschreibungGetList
	// um anschlieÃend die Details zu dieser Ausschreibung zu laden
	IAusschreibungenElement tempAusschreibung = wdContext.nodeAusschreibungen().currentAusschreibungenElement();
	tempAusschreibung.setSubmissionsnummer( wdContext.currentAusschreibungenAusschreibungsplattformReportGetListElement().getSubmissionsnummer() );
    
	wdThis.wdGetBerichteController().ladeAusschreibung();
	if( !wdComponentAPI.getMessageManager().hasExceptions() )
	{
	  // Zur Detail View navigieren
	  // der Status RELEASED impliziert, dass alle Felder Read-Only dargestellt werden, dies war
	  // Vorgabe fuer das Berichtswesen
	  wdContext.currentAusschreibungElement().setStatus(Ausschreibung.FRONTEND_STATUS_RELEASED);
	  wdThis.wdGetBerichteController().createAusplattformController();    
	  wdThis.wdFirePlugToDetailView(  );
	}
    //@@end
  }

