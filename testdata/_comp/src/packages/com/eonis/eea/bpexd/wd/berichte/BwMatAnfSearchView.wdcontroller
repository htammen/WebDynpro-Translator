  //@@begin javadoc:onActionsearchBySelection(ServerEvent)
  /** Suche der Materialanforderungen starten. */
  //@@end
  public void onActionsearchBySelection(com.sap.tc.webdynpro.progmodel.api.IWDCustomEvent wdEvent )
  {
    //@@begin onActionsearchBySelection(ServerEvent)
    
    // Ausgewählte Lagerorte in das Model-Input-Objekt des ReportGetList BP stecken.
    List inputLagerorte = wdContext.currentInputMaterialAnforderungDLLOReportGetListElement().modelObject().getLagerorte();
    inputLagerorte.clear();
    for (int i = 0; i < wdContext.nodeLagerOrteMatabrechnungGetStorageLocation().size(); i++) {
      if(wdContext.nodeLagerOrteMatabrechnungGetStorageLocation().getLagerOrteMatabrechnungGetStorageLocationElementAt(i).getSelected()){
        Lagerort lo = new Lagerort();
        lo.setWerk(wdContext.nodeLagerOrteMatabrechnungGetStorageLocation().getLagerOrteMatabrechnungGetStorageLocationElementAt(i).getWerk());
        lo.setLagerort(wdContext.nodeLagerOrteMatabrechnungGetStorageLocation().getLagerOrteMatabrechnungGetStorageLocationElementAt(i).getLagerort());
        inputLagerorte.add( lo );
      }
    }
    
    
    wdThis.wdGetBerichteController().createMatAnfComponent();
    wdThis.wdGetBerichteController().getMaterialAnforderungDLLOReportList();
    if(    !wdContext.currentGlobalModelDataElement().getCancelBusinessProcess()
        && !wdComponentAPI.getMessageManager().hasExceptions() ) {
      wdThis.wdFirePlugToResultView();
    }
    //@@end
  }

