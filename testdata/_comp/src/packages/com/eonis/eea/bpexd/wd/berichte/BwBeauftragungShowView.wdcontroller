  //@@begin javadoc:addEntries()
	/** Füllt den Entry Context Node aus den gelesenen Bestellungen und Bestellpositionen.
	 * Der Entry Node wird im TreeTable Control angezeigt. */
  //@@end
  public void addEntries( com.eonis.eea.bpexd.wd.berichte.wdp.IPrivateBwBeauftragungShowView.IEntryNode node, java.lang.String parentId )
  {
    //@@begin addEntries()
		String methodName = "addEntries";
		logger.entering(cat, methodName);
		try {
			IEntryElement newEntryElement, newPosEntryElement;
			DateFormat df = DateFormat.getDateInstance();
			NumberFormat nf = NumberFormat.getNumberInstance(WDResourceHandler.getCurrentSessionLocale());
			if (nf instanceof DecimalFormat) {
				((DecimalFormat) nf).applyPattern("#,##0.000");
			}

			for (int i = 0; i < wdContext.nodeBerichtswesenBestellungen().size(); i++) {
				Beauftragung beauftragung = wdContext.nodeBerichtswesenBestellungen().getBerichtswesenBestellungenElementAt(i).modelObject();
				// Neues Entry Element erzeugen
				newEntryElement = node.createEntryElement();
				node.addElement(newEntryElement);

				// Entry-Element mit Daten aus der Bestellung füllen.
				newEntryElement.setBestellNr(beauftragung.getBeauftragungsNr());
				newEntryElement.setDatum((beauftragung.getBestellDatum() == null ? "" : df.format(beauftragung.getBestellDatum())));
				newEntryElement.setEinkaeufer(beauftragung.getAnforderer());
				newEntryElement.setGesBestellwert(nf.format(beauftragung.getBestellwert().doubleValue()));
				newEntryElement.setWaehrung(beauftragung.getCurrency());
				newEntryElement.setAufmassart(beauftragung.getKurztext());
				newEntryElement.setBeauftragungsPosition(null);

				newEntryElement.setIsLeaf(false);
				newEntryElement.setIsPrintEnabled(true);

				for (int j = 0; j < beauftragung.getBeauftragungsPosition().size(); j++) {
					BeauftragungsPosition beauftragungsPosition = (BeauftragungsPosition) beauftragung.getBeauftragungsPosition().get(j);

					// Neues Entry Element erzeugen
					newPosEntryElement = newEntryElement.nodeChildEntries().createEntryElement();
					newEntryElement.nodeChildEntries().addElement(newPosEntryElement);

					// Entry-Element mit Daten aus der Bestellposition füllen.
					newPosEntryElement.setPos(beauftragungsPosition.getPosNr());
					newPosEntryElement.setPosKurztext(beauftragungsPosition.getShorttext());
					newPosEntryElement.setBestellNr(beauftragung.getBeauftragungsNr());
					newPosEntryElement.setBeauftragungsPosition(beauftragungsPosition);
					newPosEntryElement.setAufmassart(beauftragung.getAufmassArt());
					newPosEntryElement.setWaehrung(beauftragung.getCurrency());

					newPosEntryElement.setIsLeaf(true);
					newPosEntryElement.setIsPrintEnabled(false);
				}
			}
		} finally {
			logger.exiting(methodName);
		}
    //@@end
  }

  //@@begin javadoc:loadDetailData()
	/** Laedt die Detaildaten in den Context Node Position und laedt dann die Liste der Leistungserfassungs-
	 * blaetter aus dem Backend nach. */
  //@@end
  public void loadDetailData( com.eonis.eea.bpexd.wd.berichte.wdp.IPrivateBwBeauftragungShowView.IEntryElement element )
  {
    //@@begin loadDetailData()
		String methodName = "loadDetailData";
		logger.entering(cat, methodName);
		try {
			IPositionElement pos = wdContext.currentPositionElement();
			DateFormat df = DateFormat.getDateInstance();

			// Ist Position
			if (element.getIsLeaf() && element.getBeauftragungsPosition() != null) {
				controlVisibility(VISIBILITY_MODE_SHOW_POSITION);
				logger.debugT(cat, "Detaildaten einer Bestellposition werden angezeigt.");
				pos.setPosVisible(WDVisibility.VISIBLE);
				pos.setPosNr(element.getBeauftragungsPosition().getPosNr());
				pos.setKurztext(element.getBeauftragungsPosition().getShorttext());
				pos.setKontrakt(element.getBeauftragungsPosition().getKontrakt());
				pos.setLieferdatum((element.getBeauftragungsPosition().getLieferdatum() == null ? "" : df.format(element.getBeauftragungsPosition().getLieferdatum())));
				pos.setBedarfsnummer(element.getBeauftragungsPosition().getBedarfsnummer());
				pos.setDisponummer(element.getBeauftragungsPosition().getDisponummer());
				pos.setAufmassart(element.getAufmassart());
				if (element.getBeauftragungsPosition().getEndErfassungsKennzeichen() == null) {
					element.getBeauftragungsPosition().setEndErfassungsKennzeichen(Boolean.FALSE);
				}
				pos.setAufmasskenz(element.getBeauftragungsPosition().getEndErfassungsKennzeichen().booleanValue());
				pos.setWaehrung(element.getWaehrung());

				// Lesen der Leistungsaufmassblätter zu der Beauftragungsposition. Diese werden 
				// über den Business Process LeistungserfassungsblattGetList gelesen und im
				// Context Node leistungsErfassungsBlaetter abgelegt.
				wdContext.currentInputLeistungserfassungsblattGetListElement().setBeauftragungsNumber(element.getBestellNr());
				wdContext.currentInputLeistungserfassungsblattGetListElement().setPositionsNumber(element.getBeauftragungsPosition().getPosNr());
				wdThis.wdGetBerichteController().getLeistungserfassungsblattList();
			}
			// Ist Bestellung
			else {
				logger.debugT(cat, "Detaildaten einer Bestellung werden angezeigt.");
				pos.setPosVisible(WDVisibility.NONE);
				controlVisibility(VISIBILITY_MODE_SHOW_RESULT);
			}
		} finally {
			logger.exiting(methodName);
		}
    //@@end
  }

