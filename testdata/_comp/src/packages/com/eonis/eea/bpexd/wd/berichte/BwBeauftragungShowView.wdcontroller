  //@@begin javadoc:onActionPrintReport(ServerEvent)
	/** Declared validating event handler. */
  //@@end
  public void onActionPrintReport(com.sap.tc.webdynpro.progmodel.api.IWDCustomEvent wdEvent )
  {
    //@@begin onActionPrintReport(ServerEvent)
		String methodName = "onActionPrintReport";
		logger.entering(cat, methodName);
		try {
			ArrayList bestellungen = getSelectedBestellPosForPrint();
			// Sind Bestellungen zum Drucken markiert dann extra Fenster öffnen
			if (bestellungen.size() > 0) {
				wdThis.wdGetBerichteController().getSelectedBestellungenReport(bestellungen);

				bestellDataToPrintData();
                // dummyPdfData();
				wdThis.wdGetBerichteController().generateServletURL();

				// Company-Daten setzen
				ICompaniesElement modelCompany = wdContext.nodeCompanies().currentCompaniesElement();
				if (modelCompany.getShortText() != null) {
					wdContext.currentDataSourceElement().setCompanyText(modelCompany.getShortText());
				}
				if (modelCompany.getLogoUrl() != null) {
					wdContext.currentDataSourceElement().setCompany(modelCompany.getLogoUrl());
				}

				// Popup-Fenster erzeugen und anzeigen
				IWDWindowInfo popupBeauftragungPrintViewInfo = wdComponentAPI.getComponentInfo().findInWindows("PopupBeauftragungPrint");
				if (popupBeauftragungPrintViewInfo != null) {
					IWDWindow popupBeauftragungPrintView = wdComponentAPI.getWindowManager().createWindow(popupBeauftragungPrintViewInfo, true);
					popupBeauftragungPrintView.setWindowPosition(WDWindowPos.CENTER);
					popupBeauftragungPrintView.open();
					wdContext.currentComponentDataElement().setPopupBeauftragungPrintView(popupBeauftragungPrintView);
				}
			} else {
				wdComponentAPI.getMessageManager().reportMessage(IMessageBerichte.KEINE_BEAUFTRAGUNG_MARKIERT, null, false);
			}
		} finally {
			logger.exiting(methodName);
		}

    //@@end
  }

