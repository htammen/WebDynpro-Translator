  //@@begin javadoc:controlVisibility()
	/** Declared method. */
  //@@end
  public void controlVisibility( int mode )
  {
    //@@begin controlVisibility()
		switch (mode) {
			case VISIBILITY_MODE_SHOW_RESULT :
				wdContext.currentLocalDataElement().setBestellungenTableVisible(WDVisibility.VISIBLE);
				wdContext.currentLocalDataElement().setLeistungsaufmassTableVisible(WDVisibility.NONE);
				wdContext.currentLocalDataElement().setLeistungsAufmassDetailVisible(WDVisibility.NONE);

				break;

			case VISIBILITY_MODE_SHOW_POSITION :
				wdContext.currentLocalDataElement().setBestellungenTableVisible(WDVisibility.VISIBLE);
				wdContext.currentLocalDataElement().setLeistungsaufmassTableVisible(WDVisibility.VISIBLE);
				wdContext.currentLocalDataElement().setLeistungsAufmassDetailVisible(WDVisibility.NONE);
				break;
			case VISIBILITY_MODE_SHOW_AUFMASS_DETAIL :
				wdContext.currentLocalDataElement().setBestellungenTableVisible(WDVisibility.NONE);
				wdContext.currentLocalDataElement().setLeistungsaufmassTableVisible(WDVisibility.NONE);
				wdContext.currentLocalDataElement().setLeistungsAufmassDetailVisible(WDVisibility.VISIBLE);
				break;
		}

    //@@end
  }

