	private IEntryElement getSelectedTreeElement(IEntryElement elem) {
		// Beim ersten Aufruf muss das currentEntryElement geholt werden. Dies ist eigentlich immer
		// der root Knoten. Darauf baut jedenfalls diese Methode auf.
		if (elem == null) {
			elem = wdContext.currentEntryElement();
		}
		// Solange suchen bis kein currentChildNodeElement mehr gefunden wird.
		if (elem.currentChildEntriesElement() == null) {
			return elem;
		} else {
			// !!! Rekursion !!!
			return getSelectedTreeElement(elem.currentChildEntriesElement());
		}
	}

	/**
	 * Durchsucht die Liste der Bestellungen und überträgt die selektierten Best. 
	 * in die PDF-Datasource.
	 */
	private ArrayList getSelectedBestellPosForPrint() {
		ArrayList bestellungen = new ArrayList();
		IEntryNode eNode = wdContext.nodeEntry();
		int size = eNode.size();

		for (int i = 0; i < size; i++) {
			IEntryElement elem = eNode.getEntryElementAt(i);
			// Bestellung soll gedruckt werden
			if (elem.getIsPrintActive()) {
				Beauftragung b = new Beauftragung();
				b.setBeauftragungsNr(elem.getBestellNr());
				b.setAnforderer(elem.getEinkaeufer());
				if (elem.getDatum() != null) {
					b.setBestellDatum(convertStringToDate(elem.getDatum()));
				}
				NumberFormat nf = NumberFormat.getNumberInstance(WDResourceHandler.getCurrentSessionLocale());
				BigDecimal gesBestellWert = null;
				try {
					gesBestellWert = new BigDecimal(nf.parse(elem.getGesBestellwert()).doubleValue());
				} catch (ParseException pEx) {
					gesBestellWert = new BigDecimal(0.0);
					logger.traceThrowableT(Severity.ERROR, "NumberFormat Exception", pEx);
				}
				b.setBestellwert(gesBestellWert);
				b.setCurrency(elem.getWaehrung());
				ArrayList bestellPos = new ArrayList();
				// Kindelement Bestellpositionen
				IEntryNode posNode = elem.nodeChildEntries();
				int posSize = posNode.size();
				for (int j = 0; j < posSize; j++) {
					IEntryElement posElem = posNode.getEntryElementAt(j);
					BeauftragungsPosition bPos = new BeauftragungsPosition();
					bPos.setPosNr(posElem.getPos());
					bPos.setShorttext(posElem.getPosKurztext());
					if (posElem.getDatum() != null) {
						bPos.setLieferdatum(convertStringToDate(posElem.getDatum()));
					}

					// Ist Position
					if (null != elem.getChildEntriesElementAt(j))
						//if (elem.getIsLeaf() && elem.getBeauftragungsPosition() != null )
						{
						bPos.setKontrakt(elem.getChildEntriesElementAt(j).getBeauftragungsPosition().getKontrakt());
						bPos.setLieferdatum(elem.getChildEntriesElementAt(j).getBeauftragungsPosition().getLieferdatum());
					}

					// Kontrakt setzenbPos.setKontrakt();				
					bestellPos.add(bPos);
				}
				b.setBeauftragungsPosition(bestellPos);
				bestellungen.add(b);
			}
		}
		return bestellungen;
	}

	private void dummyPdfData() {
		wdContext.nodeBestellung().invalidate();

		// Bestellung

		IBestellungElement bestellung = (IBestellungElement) createChild(wdContext.nodeBestellung());
		bestellung.setBestellnummer("bestellung1");

		// Position

		IPosElement pos = (IPosElement) createChild(bestellung.nodePos());
		pos.setPosNr("pos1");

		// LEBlatt 1

		ILEBlattElement leBlatt1 = (ILEBlattElement) createChild(pos.nodeLEBlatt());
		leBlatt1.setLeBlattNr("LEBlatt 1");

		// LEBlatt 2

		ILEBlattElement leBlatt2 = (ILEBlattElement) createChild(pos.nodeLEBlatt());
		leBlatt2.setLeBlattNr("LEBlatt 2");

	}

	private IWDNodeElement createChild(IWDNode parent) {
		IWDNodeElement child = parent.createElement();
		parent.addElement(child);
		return child;
	}

	/**
	 * Bereitet die Context-Daten für die PDF Erzeugung auf.
	 */
	private void bestellDataToPrintData() {
		wdContext.nodeBestellung().invalidate();
		GetSelectedBestellungenReport model = null;
		if (wdContext.currentGetSelectedBestellungenReportElement() != null && wdContext.currentGetSelectedBestellungenReportElement().modelObject() != null) {
			model = wdContext.currentGetSelectedBestellungenReportElement().modelObject();
		}

		if (model != null) {
			//IBeauftragungenOutputNode modelBestNode = wdContext.nodeBeauftragungenOutput();
			List modelBestellungen = model.getOutput().getBestellungen();

			// Fuer alle Bestellungen aus dem Model-Node	
			if (modelBestellungen != null) {
				Iterator iterBestellungen = modelBestellungen.iterator();
				while (iterBestellungen.hasNext()) {
					Beauftragung modelBestElem = (Beauftragung) iterBestellungen.next();
					// Anlegen einer Bestellung im ValueNode
					IBestellungNode bestNode = wdContext.nodeBestellung();
					IBestellungElement bestElem = bestNode.createBestellungElement();
					bestNode.addElement(bestElem);
					bestElem.setBestellnummer(modelBestElem.getBeauftragungsNr());
					bestElem.setEinkaeufer(modelBestElem.getAnforderer());
					if (modelBestElem.getBestellDatum() != null) {
						bestElem.setBestelldatum(modelBestElem.getBestellDatum());
					}
					if (modelBestElem.getBestellwert() != null) {
						bestElem.setGesBestellwert(modelBestElem.getBestellwert());
					}
					bestElem.setWaehrung(modelBestElem.getCurrency());
					bestElem.setAufmassart(modelBestElem.getAufmassArt());
					// Fuer alle Bestellpositionen aus dem Modelnode einen Eintrag zum ValueNode (Bestellung + Position) hinzufuegen
					ArrayList modelBestPos = modelBestElem.getBeauftragungsPosition();
					if (modelBestPos != null) {
						for (int indexBestPos = 0; indexBestPos < modelBestPos.size(); indexBestPos++) {
							BeauftragungsPosition modelPosElem = (BeauftragungsPosition) modelBestPos.get(indexBestPos);

							// Hinzufuegen der Position zur Bestellung
							IPosNode posNode = bestElem.nodePos();
							IPosElement posElem = posNode.createPosElement();
							posNode.addElement(posElem);
							posElem.setPosNr(modelPosElem.getPosNr());
							posElem.setPosKurztext(modelPosElem.getShorttext());
							if (modelPosElem.getLieferdatum() != null) {
								posElem.setLieferdatum(modelPosElem.getLieferdatum());
							}
							posElem.setKontrakt(modelPosElem.getKontrakt());

							// Hinzufuegen aller LE-Blaetter der ModelBestellPosition zum ValueNode
							ArrayList modelLEBlaetter = modelPosElem.getLeistungsAufmassBlatt();
							
							if (modelLEBlaetter != null) {
								ILEBlattNode leNode = posElem.nodeLEBlatt();
								for (int indexLEBlaetter = 0; indexLEBlaetter < modelLEBlaetter.size(); indexLEBlaetter++) {
									LeistungsErfassungsblatt modelLeElem = (LeistungsErfassungsblatt) modelLEBlaetter.get(indexLEBlaetter);

									// Anlegen eines LE-Blatt im ValueNode

									ILEBlattElement leElem = leNode.createLEBlattElement();
									leNode.addElement(leElem);
									fillLEBlatt(leElem, modelLeElem);
								}
							}
						}
					}
				}
			}
		}
	}
	private void fillLEBlatt(ILEBlattElement leElem, LeistungsErfassungsblatt modelLeElem) {
		leElem.setLeBlattNr(modelLeElem.getLstErfBlattNr());
		leElem.setAufmassNr(modelLeElem.getLstAufmassNr());
		if (modelLeElem.getAbnahmeDatum() != null) {
			leElem.setAbnahmeDatum(modelLeElem.getAbnahmeDatum());
		}
		leElem.setExtNr(modelLeElem.getExterneNummer());
		leElem.setExtBearbeiter(modelLeElem.getExternerSachbearbeiter());
		leElem.setLeistOrt(modelLeElem.getLeistungsOrt());
		if (modelLeElem.getLstErfBlattWert() != null) {
			leElem.setWertLEBlatt(modelLeElem.getLstErfBlattWert());
		}
		leElem.setWaehrung(modelLeElem.getWaehrung());
		if (modelLeElem.getEndkennzeichen() != null) {
			leElem.setEnderfKz(modelLeElem.getEndkennzeichen().booleanValue());
		} else {
			leElem.setEnderfKz(false);
		}

		if (modelLeElem.getGutschriftBuchungsDatum() != null) {
			leElem.setBuchungsdatumGutschrift(modelLeElem.getGutschriftBuchungsDatum());
		}

		if (modelLeElem.getGutschriftNr() != null) {
			leElem.setNrGutschrift(modelLeElem.getGutschriftNr());
		}
	}
	private Date convertStringToDate(String date) {
		Date result = null;
		StringTokenizer strToken = new StringTokenizer(date, ".");
		String day = strToken.nextToken();
		String month = strToken.nextToken();
		String year = strToken.nextToken();
		result = Date.valueOf(year + "-" + month + "-" + day);
		return result;
	}
