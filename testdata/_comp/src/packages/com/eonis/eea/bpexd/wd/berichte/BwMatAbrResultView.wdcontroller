			if(lagerbestand.getVerfallsdatum() == null
				|| !sdf.format(lagerbestand.getVerfallsdatum()).matches( filterValue )) {
				// Objekt in die Schattentabelle kopieren
				ILagerbestandShadowElement shadowElem = wdContext.nodeLagerbestandShadow().createLagerbestandShadowElement();
				shadowElem.setObject( lagerbestand );
				wdContext.nodeLagerbestandShadow().addElement( shadowElem );
      
				wdContext.nodeLagerbestandList().removeElement( wdContext.nodeLagerbestandList().getLagerbestandListElementAt( i ) );
			}
		}
	}
    
    //@@end
  }

