	/**
	 * Aufbereiten des Filterwertes für StringFelder.
	 * * wird durch .* ersetzt. Ansonsten sind alle regulären Ausdrücke erlaubt.
	 */
	private String prepareStringFilterValue( String originalValue ) {
		//String value = wdContext.currentFilterValuesElement().getBeauftragungsNr();
    	String retVal = originalValue.replaceAll( "\\*", ".*" );
    	retVal = retVal.replace(',', '.');
    	 
		return retVal; 
	}
	
	/**
	 * Aufbereiten des Filterwertes für Datumsfelder.</br>
	 * "06" wird in "2006" (vorangestelltes 20) umgewandelt</br>
	 * Fehlt die Jahreszahl wird das aktuelle Jahr angehängt</br> 
	 */
	private String prepareDateFilterValue( String originalValue ) {
		StringBuffer strBuf = new StringBuffer( originalValue );
    
		// Ist mind. 1 Punkt im String enthalten
    
		return strBuf.toString();
	}
	
  //@@end
