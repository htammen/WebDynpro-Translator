  //@@begin javadoc:onActionPrintReport(ServerEvent)
	/** Declared validating event handler. */
  //@@end
  public void onActionPrintReport(com.sap.tc.webdynpro.progmodel.api.IWDCustomEvent wdEvent )
  {
    //@@begin onActionPrintReport(ServerEvent)
		String methodName = "onActionPrintReport";
		logger.entering(cat, methodName);
		try {
			wdContext.nodeDataSourceMatAbr().invalidate();
			// Sind Bestellungen zum Drucken markiert dann extra Fenster öffnen
			if (printActivated2DataSource()) {

				wdThis.wdGetBerichteController().generateServletURL();
				// Company-Daten setzen
				ICompaniesElement modelCompany = wdContext.nodeCompanies().currentCompaniesElement();
				if (modelCompany.getShortText() != null) {
					wdContext.currentDataSourceMatAbrElement().setUnternehmen(modelCompany.getShortText());
				}
				if (modelCompany.getLogoUrl() != null) {
					wdContext.currentDataSourceMatAbrElement().setLogoUrl(modelCompany.getLogoUrl());
				}
				String serviceProvider = "";
				try {
					serviceProvider = ((UnEKOrgDL) ((UnEKOrg) modelCompany.modelObject().getUnEKOrgList().get(0)).getUnEKOrgDLList().get(0)).getDienstleister().getShorttext();
				} catch (NullPointerException nEx) {
					// NullpointerException wird hier abgefangen. Ist nicht so kritisch, wenn der Dienstleistername
					// nicht im Bericht steht.
					logger.infoT(cat, new StringBuffer("Error while determining the ServiceProvider in report ").append("'Materialanforderung auf DL-LO' ").append(nEx.getMessage()).toString());
				}
				wdContext.currentDataSourceMatAbrElement().setDienstleister(serviceProvider);

				// Popup-Fenster erzeugen und anzeigen
				IWDWindowInfo popupPrintViewInfo = wdComponentAPI.getComponentInfo().findInWindows("PopupMatAbrPrint");
				if (popupPrintViewInfo != null) {
					IWDWindow popupPrintView = wdComponentAPI.getWindowManager().createWindow(popupPrintViewInfo, true);
					popupPrintView.setWindowPosition(WDWindowPos.CENTER);
					popupPrintView.open();
					wdContext.currentComponentDataElement().setPopupMatAbrPrintView(popupPrintView);
				}
			} else {
				wdComponentAPI.getMessageManager().reportMessage(IMessageBerichte.KEINE_BEAUFTRAGUNG_MARKIERT, null, false);
			}
		} finally {
			logger.exiting(methodName);
		}
    //@@end
  }

