  //@@begin javadoc:printActivated2DataSource()
	/** Declared method. */
  //@@end
  public boolean printActivated2DataSource( )
  {
    //@@begin printActivated2DataSource()

		for (int i = 0; i < wdContext.nodeLagerbestandList().size(); i++) {
			ILagerbestandListElement element = wdContext.nodeLagerbestandList().getLagerbestandListElementAt(i);

			// falls für den Druck markiert
			if (element.getIsPrintActive()) {

				IBestandElement newPrintElement = wdContext.nodeBestand().createBestandElement();
				newPrintElement.setWerk(element.getWerk());
				newPrintElement.setDllo(element.getDlLo());
				newPrintElement.setBezeichnung(element.getBezeichnung());
				newPrintElement.setBezeichnung(element.getBezeichnung());
				newPrintElement.setChargenbezeichnung(element.getChargenbezeichnung());
				newPrintElement.setChargenNr(element.getChargennummer());
				newPrintElement.setFreierBestand(element.getFreiVerwendbarerBestand());
				newPrintElement.setMaterialnummer(element.getMaterialnummer());
				newPrintElement.setMengeneinheit(element.getMengeneinheit());
				newPrintElement.setReservierterBestand(element.getReservierterBestand());

				if (element.getVerfallsdatum() != null) {
					SimpleDateFormat sdf = new SimpleDateFormat("dd.MM.yyyy");
					newPrintElement.setVerfallsdatum(sdf.format(element.getVerfallsdatum()));
				}

				// Data Source hinzufügen
				wdContext.nodeBestand().addElement(newPrintElement);
			}
		}
		return wdContext.nodeBestand().size() > 0;
    //@@end
  }

