  //@@begin javadoc:onActionPrintReport(ServerEvent)
  /** Declared validating event handler. */
  //@@end
  public void onActionPrintReport(com.sap.tc.webdynpro.progmodel.api.IWDCustomEvent wdEvent )
  {
    //@@begin onActionPrintReport(ServerEvent)
    copyMatAnfToPrintData();
    
    wdThis.wdGetBerichteController().generateServletURL();
    // Company-Daten setzen
    ICompaniesElement modelCompany = wdContext.nodeCompanies().currentCompaniesElement();
    if (modelCompany.getShortText() != null){
      wdContext.currentDSMatAnfElement().setCompanyText(modelCompany.getShortText());
    }
    if (modelCompany.getLogoUrl() != null){
      wdContext.currentDSMatAnfElement().setCompany(modelCompany.getLogoUrl());
    }
    String serviceProvider = "";
    try {
      serviceProvider = ((UnEKOrgDL)((UnEKOrg)modelCompany.modelObject().getUnEKOrgList().get(0))
                                                  .getUnEKOrgDLList().get(0)).getDienstleister().getShorttext();
    } catch( NullPointerException nEx ) {
      // NullpointerException wird hier abgefangen. Ist nicht so kritisch, wenn der Dienstleistername
      // nicht im Bericht steht.
      logger.infoT( cat, new StringBuffer( "Error while determining the ServiceProvider in report ")
                              .append( "'Materialanforderung auf DL-LO' " )
                              .append( nEx.getMessage() ).toString() );
    }
    wdContext.currentDSMatAnfElement().setServiceProvider( serviceProvider );
    

    // Popup-Fenster erzeugen und anzeigen
    IWDWindowInfo popupPrintViewInfo = wdComponentAPI.getComponentInfo().findInWindows( "PopupMatAnfPrint" );
    if( popupPrintViewInfo != null ) {
      IWDWindow popupPrintView = wdComponentAPI.getWindowManager().createWindow( popupPrintViewInfo, true );
      popupPrintView.setWindowPosition( WDWindowPos.CENTER );
      popupPrintView.open();
      wdContext.currentComponentDataElement().setPopupBeauftragungPrintView( popupPrintView );
    }

    //@@end
  }

  //@@begin javadoc:onActionselectAllItemsForPrint(ServerEvent)
  /** Selektiert alle EintrÃ¤ge der Tabelle fÃ¼r den Druck. */
  //@@end
  public void onActionselectAllItemsForPrint(com.sap.tc.webdynpro.progmodel.api.IWDCustomEvent wdEvent )
  {
    //@@begin onActionselectAllItemsForPrint(ServerEvent)
    IMaterialAnforderungenDLLOReportElement matAnfElem;
    for( int i = 0; i < wdContext.nodeMaterialAnforderungenDLLOReport().size(); i++ ) {
      matAnfElem = wdContext.nodeMaterialAnforderungenDLLOReport().getMaterialAnforderungenDLLOReportElementAt( i );
      
      // Es werden nur solche EintrÃ¤ge markiert, die eine Reservierungsnummer enthalten.
      if( matAnfElem.getReservierungsNr() != null && matAnfElem.getReservierungsNr().length() > 0 ) {
        // Toggeln des aktuellen Wertes
        matAnfElem.setIsPrintActive( !matAnfElem.getIsPrintActive() );
      }
    }
    //@@end
  }

