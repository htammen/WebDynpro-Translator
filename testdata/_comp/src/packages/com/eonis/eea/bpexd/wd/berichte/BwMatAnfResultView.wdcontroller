   * by the Web Dynpro Designtime or the Web Dynpro Runtime. 
   */
  //@@begin others
  Category cat = Category.getCategory( Category.APPLICATIONS, ApplicationConstants.LOGGING_CATEGORY_WDBERICHTE );

  /**
   * Kopiert die zum Druck markierten Materialanforderungsdaten in den Context-Node
   * fÃ¼r die PDF-Ausgabe.
   */
  private void copyMatAnfToPrintData() {
    String methodName = "copyMatAnfToPrintData";
    logger.entering(cat, methodName);
    try
    {
      IMaterialAnforderungenDLLOReportElement matAnfElem;
      IMatAnfHeaderElement matAnfHeader;
    
      // Alles was bisher in der Datasource fÃ¼r den PDF-Bericht steht, lÃ¶schen
      wdContext.nodeMatAnfHeader().invalidate();
    
      for( int i=0; i < wdContext.nodeMaterialAnforderungenDLLOReport().size(); i++ ) {
        matAnfElem = wdContext.nodeMaterialAnforderungenDLLOReport().getMaterialAnforderungenDLLOReportElementAt( i );
        if( matAnfElem.getIsPrintActive() ) {
          matAnfHeader = wdContext.createMatAnfHeaderElement();
        
          matAnfHeader.setKURZTEXT( matAnfElem.getKurzText() );
          matAnfHeader.setLAGERORT( matAnfElem.getLagerOrt() );
		  matAnfHeader.setLAGERORT_DESC( matAnfElem.getLagerOrtText() );
          matAnfHeader.setMATAUFMASSNR( matAnfElem.getMatAufmassNr() );
          matAnfHeader.setRESERVIERUNGSDATUM( matAnfElem.getReservierungsDatum() );
          matAnfHeader.setRESERVIERUNGSNR( matAnfElem.getReservierungsNr() );
          matAnfHeader.setWARENEMPFAENGERNR( matAnfElem.getWarenEmpfaengerNr() );
          matAnfHeader.setWERK( matAnfElem.getWerk() );
          matAnfHeader.setWUNSCHTERMIN( matAnfElem.getWunschTermin() );
        
          wdContext.nodeMatAnfHeader().addElement( matAnfHeader );
          
          if( logger.beDebug() ) {
            logger.debugT( cat, new StringBuffer( "Materialanforderungsheader zum PDF-Context hinzugefÃ¼gt. ")
                                      .append( "Kurztext: ").append( matAnfHeader.getKURZTEXT() )
                                      .append( ", Lagerort: ").append( matAnfHeader.getLAGERORT() )
                                      .append( ", Materialaufmassnr: ").append( matAnfHeader.getMATAUFMASSNR() )
                                      .append( ", Reservierungsdatum: ").append( matAnfHeader.getRESERVIERUNGSDATUM().toString() )
                                      .append( ", Reservierungsnr: ").append( matAnfHeader.getRESERVIERUNGSNR() )
                                      .append( ", WarenempfÃ¤nger: ").append( matAnfHeader.getWARENEMPFAENGERNR() )
                                      .append( ", Werk: ").append( matAnfHeader.getWERK() )
                                      .append( ", Wunschtermin: ").append( matAnfHeader.getWUNSCHTERMIN().toString() )
                                      .toString()
                         );
          }
        
          // FÃ¼r jedes ausgewÃ¤hlte Materialaufmass werden jetzt die Detaildaten 
          // nachgelesen.
          IMatAnfPosElement matAnfPos = null;
          wdThis.wdGetBerichteController().getReservierungDetailByResNr( matAnfElem.getReservierungsNr() );
          for( int j=0; j<wdContext.nodeReservierungsPos().size(); j++) {
            IReservierungsPosElement posElem = 
                    wdContext.nodeReservierungsPos().getReservierungsPosElementAt( j );
            matAnfPos = matAnfHeader.nodeMatAnfPos().createMatAnfPosElement();
          
            matAnfPos.setBEDARFSMENGE( posElem.getBedarfsMenge() );
            matAnfPos.setENTNAHMEMENGE( posElem.getEntnommeneMenge() );
            matAnfPos.setKURZTEXT( posElem.getMaterialText() );
            matAnfPos.setMATERIALNR( posElem.getMaterialNr() );
            matAnfPos.setMENGENEINHEIT( posElem.getMengenEinheit() );
            matAnfPos.setSTATUS( posElem.getStatus() );

            matAnfHeader.nodeMatAnfPos().addElement( matAnfPos );          

            if( logger.beDebug() ) {
              logger.debugT( cat, new StringBuffer( "Materialanforderungsposition zum PDF-Context hinzugefÃ¼gt. ")
                                        .append( "Bedarfsmenge: ").append( matAnfPos.getBEDARFSMENGE().toString() )
                                        .append( ", Entnahmenmenge: ").append( matAnfPos.getENTNAHMEMENGE().toString() )
                                        .append( ", Kurztext: ").append( matAnfPos.getKURZTEXT() )
                                        .append( ", Materialnr: ").append( matAnfPos.getMATERIALNR() )
                                        .append( ", Mengeneinheit: ").append( matAnfPos.getMENGENEINHEIT() )
                                        .append( ", Status: ").append( matAnfPos.getSTATUS() )
                                        .toString()
                           );
            }
          }
        }
      }
    } finally
    {
      logger.exiting(methodName);
    }
  }
  //@@end
}
